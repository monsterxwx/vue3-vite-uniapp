{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"pages": [
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		},
		{
			"path": "pages/manage/index",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		},
		{
			"path": "pages/test/index",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"enablePullDownRefresh": false,
		"navigationBarTitleText": "",
		"navigationBarBackgroundColor": "#0078D4",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#999999",
		"selectedColor": "#7181cb",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [
			{
				"pagePath": "pages/index/index",
				"iconPath": "static/image/index/home_off.png",
				"selectedIconPath": "static/image/index/home_on.png",
				"text": "施工管理"
			},
			{
				"pagePath": "pages/manage/index",
				"iconPath": "static/image/index/user_off.png",
				"selectedIconPath": "static/image/index/user_on.png",
				"text": "个人中心"
			}
		]
	}
}